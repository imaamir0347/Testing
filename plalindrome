#include <iostream>
#include <string>
#include <algorithm>

using namespace std;

int main()
{
    int t;
    int i=0;
    string str;
    string str2;
    cin>>t;
    while(i<t)
    {
        cin>>str;
        str2=str;
        reverse(str.begin(),str.end());
        if(str==str2)
            cout<<"YES"<<endl;
        else
            cout<<"NO"<<endl;
        i++;
    }
    return 0;
}
